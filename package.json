{
	"name": "advanced-command-handler",
	"version": "1.3.0-beta.0",
	"description": "A package to help you create a bot with only 1 main file and directories for your events and commands with some helpers classes and functions.",
	"main": "./dist/index.js",
	"author": "Ayfri",
	"license": "ISC",
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"@types/luxon": "^1.25.0",
		"@types/node": "^14.14.13",
		"discord.js": "^12.5.1",
		"discord.js-better-embed": "^1.0.3",
		"luxon": "^1.25.0"
	},
	"scripts": {
		"prettier": "prettier ./ --write",
		"compile": "tsc"
	},
	"keywords": [
		"discord",
		"command-handler",
		"discord-bot",
		"discordjs"
	],
	"homepage": "https://github.com/Ayfri/Advanced-Command-Handler",
	"maintainers": [
		"ayfri"
	],
	"repository": {
		"type": "github",
		"url": "git://github.com/Ayfri/Advanced-Command-Handler/tree/package.git"
	},
	"readme": "https://github.com/Ayfri/Advanced-Command-Handler/blob/package/README.md",
	"bugs": {
		"url": "https://github.com/Ayfri/Advanced-Command-Handler/issues"
	},
	"devDependencies": {
		"prettier": "^2.1.2",
		"typescript": "^4.1.2"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
