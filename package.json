{
	"author": "Ayfri",
	"bugs": {
		"url": "https://github.com/Ayfri/Advanced-Command-Handler/issues"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"dayjs": "^1.10.3",
		"discord.js": "^12.5.1",
		"discord.js-better-embed": "^1.1.0"
	},
	"description": "A package to help you create a bot with only 1 main file, directories for your events/commands, with some helpers classes and functions.",
	"devDependencies": {
		"@types/node": "^14.14.21",
		"eledoc": "^0.2.1",
		"prettier": "^2.2.1",
		"typedoc": "^0.20.16",
		"typescript": "^4.1.3"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=14"
	},
	"homepage": "https://github.com/Ayfri/Advanced-Command-Handler",
	"keywords": [
		"discord",
		"command-handler",
		"discord-bot",
		"discordjs"
	],
	"license": "ISC",
	"main": "./dist/index.js",
	"maintainers": [
		"ayfri"
	],
	"name": "advanced-command-handler",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"printWidth": 200,
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"readme": "https://github.com/Ayfri/Advanced-Command-Handler/blob/package/README.md",
	"repository": {
		"type": "github",
		"url": "git://github.com/Ayfri/Advanced-Command-Handler/tree/package.git"
	},
	"scripts": {
		"build": "tsc",
		"prepare": "npm run build",
		"prettier": "prettier package.json tsconfig.json README.md src tests --write",
		"genDocs": "typedoc src --out docs --name \"Advanced Command Handler\" --readme https://github.com/Ayfri/Advanced-Command-Handler/blob/master/README.md"
	},
	"types": "types/index.d.ts",
	"version": "2.2.2"
}
