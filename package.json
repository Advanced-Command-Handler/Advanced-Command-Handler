{
	"author": "Ayfri",
	"bugs": {
		"url": "https://github.com/Ayfri/Advanced-Command-Handler/issues"
	},
	"dependencies": {
		"chalk": "^4.1.1",
		"dayjs": "^1.10.5",
		"discord.js": "^12.5.3",
		"discord.js-better-embed": "^1.2.0"
	},
	"description": "A package to help you create a bot with only 1 main file, directories for your events/commands, with some helpers classes and functions.",
	"devDependencies": {
		"@types/node": "^15.6.1",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-plugin-jsdoc": "^35.0.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"prettier": "^2.3.0",
		"typescript": "^4.3.2"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=14"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 2021,
			"sourceType": "module"
		},
		"rules": {
			"tsdoc/syntax": "warn",
			"require-jsdoc": "warn",
			"jsdoc/check-tag-names": "off",
			"jsdoc/require-param-type": "off",
			"jsdoc/require-returns-type": "off"
		},
		"extends": [
			"plugin:jsdoc/recommended"
		],
		"plugins": [
			"eslint-plugin-tsdoc",
			"@typescript-eslint",
			"eslint-plugin-jsdoc"
		]
	},
	"homepage": "https://github.com/Ayfri/Advanced-Command-Handler",
	"keywords": [
		"discord",
		"command-handler",
		"discord-bot",
		"discordjs"
	],
	"license": "ISC",
	"main": "./dist/index.js",
	"maintainers": [
		"ayfri"
	],
	"name": "advanced-command-handler",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"printWidth": 200,
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"readme": "https://github.com/Ayfri/Advanced-Command-Handler/blob/package/README.md",
	"repository": {
		"type": "github",
		"url": "git://github.com/Ayfri/Advanced-Command-Handler/tree/package.git"
	},
	"scripts": {
		"build": "tsc",
		"lint": "eslint src/**/*.ts --fix",
		"prepare": "npm run build",
		"prettier": "prettier **.json **.md src tests --write"
	},
	"types": "types/index.d.ts",
	"version": "2.4.0"
}
